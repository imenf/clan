[core]
	repositoryformatversion = 0
	filemode = true
	bare = false
	logallrefupdates = true
[remote "origin"]
	url = git@github.com:imenf/clan.git
	fetch = +refs/heads/*:refs/remotes/origin/*
[branch "master"]
	remote = origin
	merge = refs/heads/master
[remote "periscop"]
	url = https://github.com/periscop/clan.git
	fetch = +refs/heads/*:refs/remotes/periscop/*
[submodule "osl"]
	url = https://github.com/periscop/openscop.git
[remote "imen"]
	url = ssh://xfor@xfor.u-strasbg.fr/git/clan
	fetch = +refs/heads/*:refs/remotes/imen/*
